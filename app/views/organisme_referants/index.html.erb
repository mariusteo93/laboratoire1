<p id="notice"><%= notice %></p>

<h1>Organisme Referants</h1>

<table>
  <thead>
    <tr>
      <th>Nom</th>
      <th>Nocivique</th>
      <th>Rue</th>
      <th>Ville</th>
      <th>Province</th>
      <th>Etat</th>
      <th>Codepostal</th>
      <th>Telephonebureau</th>
      <th>Telephonetelecopie</th>
      <th>Courriel</th>
      <th>Siteweb</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @organisme_referants.each do |organisme_referant| %>
      <tr>
        <td><%= organisme_referant.nom %></td>
        <td><%= organisme_referant.noCivique %></td>
        <td><%= organisme_referant.rue %></td>
        <td><%= organisme_referant.ville %></td>
        <td><%= organisme_referant.province %></td>
        <td><%= organisme_referant.etat %></td>
        <td><%= organisme_referant.codePostal %></td>
        <td><%= organisme_referant.telephoneBureau %></td>
        <td><%= organisme_referant.telephoneTelecopie %></td>
        <td><%= organisme_referant.courriel %></td>
        <td><%= organisme_referant.siteWeb %></td>
        <td><%= link_to 'Show', organisme_referant %></td>
        <td><%= link_to 'Edit', edit_organisme_referant_path(organisme_referant) %></td>
        <td><%= link_to 'Destroy', organisme_referant, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Organisme Referant', new_organisme_referant_path %>
